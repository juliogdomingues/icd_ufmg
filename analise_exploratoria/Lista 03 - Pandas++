    {
      "cell_type": "code",
      "execution_count": 13,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "yGr69ZfHGNrQ",
        "outputId": "38a4a5f1-ba48-4ec0-c3aa-d45982e65296"
